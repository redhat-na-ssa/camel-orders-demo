[{
  "apiVersion" : "v1",
  "kind" : "ServiceAccount",
  "metadata" : {
    "labels" : {
      "app.kubernetes.io/name" : "camel-processor",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "camel-processor"
  }
},{
  "apiVersion" : "rbac.authorization.k8s.io/v1",
  "kind" : "RoleBinding",
  "metadata" : {
    "name" : "camel-processor-view"
  },
  "roleRef" : {
    "kind" : "ClusterRole",
    "apiGroup" : "rbac.authorization.k8s.io",
    "name" : "view"
  },
  "subjects" : [ {
    "kind" : "ServiceAccount",
    "name" : "camel-processor"
  } ]
},{
  "apiVersion" : "v1",
  "kind" : "Service",
  "metadata" : {
    "labels" : {
      "app.kubernetes.io/name" : "camel-processor",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "camel-processor"
  },
  "spec" : {
    "ports" : [ {
      "name" : "http",
      "port" : 8080,
      "protocol" : "TCP",
      "targetPort" : 7070
    }, {
      "name" : "https",
      "port" : 443,
      "protocol" : "TCP",
      "targetPort" : 8443
    } ],
    "selector" : {
      "app.kubernetes.io/name" : "camel-processor"
    },
    "type" : "ClusterIP"
  }
},{
  "apiVersion" : "apps/v1",
  "kind" : "Deployment",
  "metadata" : {
    "labels" : {
      "app.kubernetes.io/name" : "camel-processor",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "camel-processor"
  },
  "spec" : {
    "replicas" : 1,
    "selector" : {
      "matchLabels" : {
        "app.kubernetes.io/name" : "camel-processor"
      }
    },
    "template" : {
      "metadata" : {
        "labels" : {
          "app.kubernetes.io/name" : "camel-processor",
          "app.kubernetes.io/managed-by" : "quarkus"
        }
      },
      "spec" : {
        "containers" : [ {
          "env" : [ {
            "name" : "KUBERNETES_NAMESPACE",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "metadata.namespace"
              }
            }
          }, {
            "name" : "JAVA_OPTIONS",
            "value" : "-Dvertx.metrics.options.enabled=true -Dvertx.metrics.options.registryName=prometheus"
          } ],
          "image" : "quay.io/redhat_naps_da/camel-processor:1.0.0-SNAPSHOT",
          "imagePullPolicy" : "Always",
          "livenessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/live",
              "port" : 7070,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "name" : "camel-processor",
          "ports" : [ {
            "containerPort" : 8443,
            "name" : "https",
            "protocol" : "TCP"
          }, {
            "containerPort" : 7070,
            "name" : "http",
            "protocol" : "TCP"
          } ],
          "readinessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/ready",
              "port" : 7070,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "resources" : {
            "limits" : {
              "cpu" : "500m",
              "memory" : "512Mi"
            },
            "requests" : {
              "cpu" : "100m",
              "memory" : "128Mi"
            }
          },
          "startupProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/started",
              "port" : 7070,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "volumeMounts" : [ {
            "mountPath" : "/deployments/config",
            "name" : "config",
            "readOnly" : false
          } ]
        } ],
        "initContainers" : [ {
          "args" : [ "pod", "-lapp.kubernetes.io/name=mysql" ],
          "image" : "quay.io/jbride/k8s-wait-for:no-root-v1.7",
          "imagePullPolicy" : "IfNotPresent",
          "name" : "check-mysql"
        }, {
          "args" : [ "pod", "-lstatefulset.kubernetes.io/pod-name=artemis-ss-0" ],
          "image" : "quay.io/jbride/k8s-wait-for:no-root-v1.7",
          "imagePullPolicy" : "IfNotPresent",
          "name" : "check-artemis"
        } ],
        "serviceAccountName" : "camel-processor",
        "volumes" : [ {
          "configMap" : {
            "defaultMode" : 420,
            "items" : [ {
              "key" : "application.properties",
              "path" : "application.properties"
            } ],
            "name" : "camel-processor",
            "optional" : false
          },
          "name" : "config"
        } ]
      }
    }
  }
}]